<?xml version="1.0"?>
<profile xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
				 xsi:noNamespaceSchemaLocation="../../../install/profiles/xml/profile.xsd" useForConfiguration="1"
				 infoUrl="http://wamwpcmis.da.wa.gov.au">
	<profileName>Western Australian Museum CMIS</profileName>
	<profileDescription>Western Australian Museum Collection Management Information System</profileDescription>
	<locales>
		<locale lang="en" country="AU">English (Australian)</locale>
	</locales>
	<lists>
		<list code="object_types" hierarchical="0" system="0" vocabulary="1">
			<labels>
				<label locale="en_AU">
					<name>Object Types</name>
				</label>
			</labels>
			<items>
				<item idno="DublinCore" enabled="1" default="0">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Cultural Heritage Record</name_singular>
							<name_plural>Cultural Heritage Records</name_plural>
						</label>
					</labels>
					<items>
						<item idno="history" enabled="1" default="0">
							<labels>
								<label locale="en_AU" preferred="1">
									<name_singular>History</name_singular>
									<name_plural>History</name_plural>
								</label>
							</labels>
						<items><item idno="history_part" enabled="1" default="0">
							<labels>
								<label locale="en_AU" preferred="1">
									<name_singular>History Part</name_singular>
									<name_plural>History Part</name_plural>
								</label>
							</labels>
							</item>
						</items>
						</item>
					</items>
				</item>
			</items>
		</list>
		<list code="history_material_types" hierarchical="1" system="1" vocabulary="1">
			<labels>
				<label locale="en_AU">
					<name>History Material Types</name>
				</label>
			</labels>
		</list>
		<list code="history_measurement_precision_types" hierarchical="0" system="1" vocabulary="0">
			<labels>
				<label locale="en_AU">
					<name>History Measurement Precision Types</name>
				</label>
			</labels>
			<items>
				<item idno="precise" enabled="1" default="1" type="concept">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Precise</name_singular>
							<name_plural>Precise</name_plural>
						</label>
					</labels>
				</item>
				<item idno="minimum" enabled="1" default="0" type="concept">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Minimum</name_singular>
							<name_plural>Minima</name_plural>
						</label>
					</labels>
				</item>
				<item idno="maximum" enabled="1" default="0" type="concept">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Maximum</name_singular>
							<name_plural>Maxima</name_plural>
						</label>
					</labels>
				</item>

				<item idno="approximate" enabled="1" default="0" type="concept">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Approximate</name_singular>
							<name_plural>Approximate</name_plural>
						</label>
					</labels>
				</item>
			</items>
		</list>
		<list code="history_classification" hierarchical="1" system="1" vocabulary="1">
			<labels>
				<label locale="en_AU">
					<name>History Classification</name>
				</label>
			</labels>
			<items>
				<item idno="unclassified" enabled="0" default="1">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Unclassified</name_singular>
							<name_plural>Unclassified</name_plural>
						</label>
					</labels>
				</item>
			</items>
		</list>
		<list code="history_measurement_types" hierarchical="0" system="1" vocabulary="0">
			<labels>
				<label locale="en_AU">
					<name>History Measurement Types</name>
				</label>
			</labels>
			<items>
				<item idno="length" enabled="1" default="1" type="concept">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Length</name_singular>
							<name_plural>Lengths</name_plural>
						</label>
					</labels>
				</item>
				<item idno="width" enabled="1" default="0" type="concept">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Width</name_singular>
							<name_plural>Widths</name_plural>
						</label>
					</labels>
				</item>
				<item idno="height" enabled="1" default="0" type="concept">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Height</name_singular>
							<name_plural>Heights</name_plural>
						</label>
					</labels>
				</item>
				<item idno="diameter" enabled="1" default="0" type="concept">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Diameter</name_singular>
							<name_plural>Diameters</name_plural>
						</label>
					</labels>
				</item>
				<item idno="weight" enabled="1" default="0" type="concept">
					<labels>
						<label locale="en_AU" preferred="1">
							<name_singular>Weight</name_singular>
							<name_plural>Weights</name_plural>
						</label>
					</labels>
				</item>
			</items>
		</list>
	</lists>
	<elementSets>
		<metadataElement code="historyMeasurementContainer" datatype="Container">
			<labels><label locale="en_AU">
				<name>Measurements(Including Type)</name>
			</label>
			</labels>
			<settings>
				<setting name="lineBreakAfterNumberOfElements">3</setting>
			</settings>
			<elements>
				<metadataElement code="history_measurement_type" list="history_measurement_types" datatype="List">
					<labels>
						<label locale="en_AU">
							<name>Measurement Type</name>
						</label>
					</labels>
					<settings>
						<setting name="doesNotTakeLocale">0</setting>
						<setting name="render">select</setting>
					</settings>
				</metadataElement>
				<metadataElement code="history_measurement_precision" datatype="List" list="history_measurement_precision_types">
					<labels><label locale="en_AU">
						<name>Precision</name>
					</label></labels>
					<settings>
						<setting name="doesNotTakeLocale">0</setting>
						<setting name="render">select</setting>
					</settings>
				</metadataElement>
				<metadataElement code="measure_display_flag" datatype="List" list="yesNo">
					<labels><label locale="en_AU">
						<name>Display Flag</name>
					</label></labels>
					<settings>
						<setting name="doesNotTakeLocale">0</setting>
						<setting name="render">select</setting>
					</settings>
				</metadataElement>
				<metadataElement code="history_measurement_length" datatype="Length">
					<labels><label locale="en_AU">
						<name>Dimension</name>
					</label></labels>
					<settings>
						<setting name="fieldWidth">25</setting>
					</settings>
				</metadataElement>
				<metadataElement code="history_measurement_weight" datatype="Weight">
				<labels><label locale="en_AU">
						<name>Weight</name>
					</label></labels>
				<settings>
					<setting name="fieldWidth">25</setting>
				</settings>
			</metadataElement>
				<metadataElement code="history_measurement_text" datatype="Text">
				<labels><label locale="en_AU">
						<name>Text Measurement</name>
					</label></labels>
				<settings>
					<setting name="fieldWidth">25</setting>
				</settings>
			</metadataElement>
				<metadataElement code="measure_notes" datatype="Text">
					<labels><label locale="en_AU">
						<name>Notes</name>
					</label></labels>
					<settings>
						<setting name="fieldWidth">150</setting>
						<setting name="fieldHeight">4</setting>
					</settings>
				</metadataElement>

			</elements>
			<typeRestrictions>
				<restriction>
					<table>ca_objects</table>
					<type>history</type>
					<includeSubtypes>1</includeSubtypes>
					<settings>
						<setting name="minAttributesPerRow">1</setting>
						<setting name="maxAttributesPerRow">255</setting>
						<setting name="minimumAttributeBundlesToDisplay">1</setting>
					</settings>
				</restriction>
			</typeRestrictions>
		</metadataElement>
		<metadataElement code="history_material" list="history_material_types" datatype="List">
			<labels>
				<label locale="en_AU">
					<name>Materials (History)</name>
				</label>
			</labels>
			<settings>
				<setting name="doesNotTakeLocale">0</setting>
				<setting name="render">horiz_hierbrowser_with_search</setting>
			</settings>
			<typeRestrictions>
				<restriction>
					<table>ca_objects</table>
					<type>history</type>
					<includeSubtypes>1</includeSubtypes>
					<settings>
						<setting name="minAttributesPerRow">1</setting>
						<setting name="maxAttributesPerRow">255</setting>
						<setting name="minimumAttributeBundlesToDisplay">1</setting>
					</settings>
				</restriction>
			</typeRestrictions>
		</metadataElement>
		<metadataElement code="department" datatype="Entities">
			<labels>
				<label locale="en_AU">
					<name>Department</name>
				</label>
			</labels>
			<settings>
				<setting name="requireValue">0</setting>
				<setting name="doesNotTakeLocale">0</setting>
				<setting name="canBeUsedInSort">0</setting>
				<setting name="canBeUsedInSearchForm">1</setting>
				<setting name="canBeUsedInDisplay">1</setting>
			</settings>
			<typeRestrictions>
				<restriction>
					<table>ca_objects</table>
					<type>history</type>
					<includeSubtypes>1</includeSubtypes>
					<settings>
						<setting name="minAttributesPerRow">1</setting>
						<setting name="maxAttributesPerRow">1</setting>
						<setting name="minimumAttributeBundlesToDisplay">1</setting>
					</settings>
				</restriction>
			</typeRestrictions>
		</metadataElement>
	</elementSets>
	<userInterfaces>
		<userInterface code="history" type="ca_objects">
			<labels>
				<label locale="en_AU">
					<name>History Editor</name>
				</label>
			</labels>
			<typeRestrictions>
				<restriction type="history"/>
				<restriction type="history_part"/>
				<restriction type="early_childhood"/>

			</typeRestrictions>
			<groupAccess>
				<permission group="admin" access="edit"/>
			</groupAccess>
			<screens>
				<screen idno="history_basic" default="1">
					<labels>
						<label locale="en_AU">
							<name>Basic Information</name>
						</label>
					</labels>
					<typeRestrictions>
						<restriction type="history"/>
						<restriction type="early_childhood"/>
					</typeRestrictions>
					<bundlePlacements>
						<placement code="ca_attribute_history_material">
							<bundle>ca_attribute_history_material</bundle>
							<settings>
								<setting name="sortDirection">ASC</setting>
								<setting name="readonly"/>
							</settings>
						</placement>
						<placement code="ca_attribute_HistoryMeasurement">
							<bundle>ca_attribute_historyMeasurementContainer</bundle>
							<settings>
								<setting name="sortDirection">ASC</setting>
								<setting name="readonly"/>
							</settings>
						</placement>
						<placement code="ca_attribute_department">
							<bundle>ca_attribute_department</bundle>
							<settings>
								<setting name="sortDirection">ASC</setting>
								<setting name="readonly"/>
							</settings>
						</placement>
					</bundlePlacements>
				</screen>
				<screen idno="history_part" default="1">
					<labels>
						<label locale="en_AU">
							<name>Part Information</name>
						</label>
					</labels>
					<typeRestrictions>
						<restriction type="history_part"/>
					</typeRestrictions>
					<bundlePlacements>
						<placement code="idno2">
							<bundle>idno</bundle>
							<settings>
								<setting name="readonly"/>
							</settings>
						</placement>
						<placement code="preferred_labels1">
							<bundle>preferred_labels</bundle>
							<settings>
								<setting name="label" locale="en_AU">Part Name</setting>
								<setting name="readonly"/>
								<setting name="usewysiwygeditor"/>
							</settings>
						</placement>
						<placement code="ca_attribute_HistoryMeasurement">
							<bundle>ca_attribute_historyMeasurementContainer</bundle>
							<settings>
								<setting name="sortDirection">ASC</setting>
								<setting name="readonly"/>
							</settings>
						</placement>
					</bundlePlacements>
				</screen>
			</screens>
		</userInterface>
	</userInterfaces>
	<relationshipTypes>
		<relationshipTable name="ca_objects_x_entities">
			<types>
				<type code="department" default="0">
					<labels>
						<label locale="en_AU">
							<typename>in department</typename>
							<typename_reverse>is department for</typename_reverse>
						</label>
					</labels>
				</type>
			</types>
		</relationshipTable>
	</relationshipTypes>
	<roles/>
	<groups/>
	<searchForms></searchForms>
</profile>
